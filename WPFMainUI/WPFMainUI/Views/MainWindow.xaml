<Window x:Class="WPFMainUI.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:prism="http://prismlibrary.com/"
        xmlns:local="clr-namespace:WPFMainUI"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="MainWindow" Height="350" Width="525" Background="{x:Null}">
    <Grid>
        <DockPanel>
            <ContentControl prism:RegionManager.RegionName="MenuRegion" DockPanel.Dock="Left"/>
            <ContentControl prism:RegionManager.RegionName="TitleRegion" DockPanel.Dock="Top"/>
            <ContentControl prism:RegionManager.RegionName="FeatureRegion"/>
        </DockPanel>
        <ContentControl prism:RegionManager.RegionName="EntireRegion"/>
    </Grid>
</Window>
